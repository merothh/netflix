<?xml version="1.0" encoding="utf-8"?>
<LinearLayout orientation="vertical" id="@id/search_fragment_container" style="@style/match"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <FrameLayout id="@id/search_prequery_container" visibility="gone" style="@style/match" />
    <include layout_width="fill_parent" layout_height="fill_parent" layout_margin="@dimen/search_instructions_container_margin" layout="@layout/simple_instructions_container" />
    <include layout="@layout/loading_and_error_inset" />
    <o.ExpandableListConnector id="@id/search_results_scrollview" layout_width="fill_parent" layout_height="fill_parent">
        <LinearLayout orientation="vertical" id="@id/vertical_layout" style="@style/match">
            <include id="@id/search_results_label_videos" visibility="gone" layout="@layout/search_results_section_header" />
            <o.RatingBar id="@id/search_results_grid_view_videos" paddingTop="6.0dip" paddingBottom="8.0dip" paddingStart="6.0dip" paddingEnd="6.0dip" style="@style/match" />
            <include id="@id/search_results_label_suggestions" visibility="gone" layout="@layout/search_results_section_header" />
            <o.RatingBar id="@id/search_results_grid_view_suggestions" style="@style/match" />
        </LinearLayout>
    </o.ExpandableListConnector>
</LinearLayout>