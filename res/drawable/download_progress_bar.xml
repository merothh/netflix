<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item id="@android:id/secondaryProgress">
        <shape shape="ring" innerRadiusRatio="6.0" thicknessRatio="20.0" useLevel="true">
            <gradient startColor="#ff999999" endColor="#ff999999" type="sweep" centerColor="#ff999999" />
        </shape>
    </item>
    <item id="@android:id/progress">
        <rotate android:fromDegrees="270.0" android:toDegrees="270.0" android:pivotX="50.0%" android:pivotY="50.0%">
            <shape shape="ring" innerRadiusRatio="3.5" useLevel="true" thickness="3.0dip">
                <rotate android:fromDegrees="0.0" android:toDegrees="360.0" android:pivotX="50.0%" android:pivotY="50.0%" />
                <gradient startColor="@color/download_button" endColor="@color/download_button" type="sweep" centerColor="@color/download_button" />
            </shape>
        </rotate>
    </item>
</layer-list>