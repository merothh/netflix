<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item id="@android:id/background">
        <shape shape="rectangle">
            <solid color="@color/white" />
            <corners radius="6.0dip" />
        </shape>
    </item>
    <item id="@android:id/secondaryProgress">
        <clip>
            <shape shape="rectangle">
                <stroke width="2.0dip" color="@color/white" />
                <solid color="@color/grey" />
                <corners radius="6.0dip" />
            </shape>
        </clip>
    </item>
    <item id="@android:id/progress">
        <clip>
            <shape shape="rectangle">
                <stroke width="2.0dip" color="@color/white" />
                <solid color="@color/brand_red" />
                <corners radius="6.0dip" />
            </shape>
        </clip>
    </item>
</layer-list>